{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error", { "endOfLine": "auto" }],
    "no-console": "off"
  },
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "env": {
    "jest": true,
    "node": true
  },
  "globals": {
    "test": "readonly",
    "expect": "readonly",
    "process": "readonly"
  }
}
